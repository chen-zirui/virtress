{
	"name":"SampleAsset",
	"path":"/sample/asset",
	"groups": [
		{
			"matchers": [
				{
					"type":"HEADER",
					"name":"SampleHeader",
					"value":"*"
				},
				{
					"type":"HEADER",
					"name":"SampleHeaderDos",
					"value":"SampleHeaderValue"
				},
				{
					"type":"PATH"
				}
			],
			"response":"{ \"sampleResponse\":\"1st sample response matching request headers and the path.\" }"
		},
		{
			"matchers": [
				{
					"type":"PATH"
				}
			],
			"response":"{ \"sampleResponse\":\"default sample response by matching the path.\" }"
		}
	]
}